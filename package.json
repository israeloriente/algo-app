{
  "name": "logiface",
  "version": "5.0.19",
  "author": "Starface",
  "homepage": "https://app.starface.pt",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "ng": "ng",
    "start": "ionic s --no-open --port 4300",
    "start:stage": "ionic s --no-open --configuration stage --port 4300",
    "start:prod": "ionic s --no-open --configuration production --port 4300",
    "build": "ng build",
    "build:stage": "ng build --configuration stage",
    "build:prod": "ng build --configuration production",
    "host": "node server.js",
    "cy:open": "node_modules/.bin/cypress open",
    "cy:run": "cypress run",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "clean": "sudo rm -rf node_modules package-lock.json && npm install && npm run start",
    "check": "npm outdated",
    "check:dev": "npm audit",
    "app:prepare": "husky install",
    "commit": "cz"
  },
  "dependencies": {
    "@angular/animations": "^18.2.13",
    "@angular/cdk": "^17.1.2",
    "@angular/cli": "^18.2.12",
    "@angular/common": "^18.2.13",
    "@angular/compiler-cli": "^18.2.13",
    "@angular/core": "^18.2.13",
    "@angular/forms": "^18.2.13",
    "@angular/material": "^17.1.2",
    "@angular/platform-browser": "^18.2.13",
    "@angular/platform-browser-dynamic": "^18.2.13",
    "@angular/pwa": "^0.5.3",
    "@angular/router": "^18.2.13",
    "@angular/service-worker": "^18.2.13",
    "@ionic/angular": "^8.4.1",
    "@ionic/angular-toolkit": "^11.0.1",
    "@ionic/pwa-elements": "^3.2.2",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@types/lodash": "^4.14.202",
    "aws-sdk": "^2.1553.0",
    "axios": "^1.7.2",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "moment-timezone": "^0.5.45",
    "ngx-image-compress": "^15.1.6",
    "ngx-webcam-controller": "^0.1.28",
    "swiper": "^11.0.6",
    "tslib": "^2.6.2",
    "zone.js": "^0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1802.12",
    "@angular-devkit/build-angular": "^18.2.12",
    "@angular-devkit/core": "^18.2.12",
    "@angular-devkit/schematics": "^18.2.12",
    "@angular/compiler": "^18.2.13",
    "@types/node": "^20.11.16",
    "ts-node": "^10.9.2",
    "typescript": "~5.5.4"
  }
